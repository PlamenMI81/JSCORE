{{> header-r}}
<div class="content">
    {{> menu-r}}
    <section id="viewMyPosts">
        <div class="post post-content">
            <h1>Your Posts</h1>
        </div>
        <div class="posts">
            {{#if posts}}
                {{#each posts}}
                    {{> row-mypost}}
                {{/each}}
            {{else}}
            <article class="post">
                <p>No posts in database</p>
            </article>
            {{/if}}
        </div>
    </section>
</div>
{{> footer}}